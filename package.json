{
    "name": "guni",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "homepage": "https://gunizzi.github.io/guni/",
    "scripts": {
        "dev": "vite",
        "dev:prod": "vite --mode production",
        "build": "tsc && vite build --mode production --outDir @dist/prod",
        "build:dev": "tsc && vite build --mode development --outDir @dist/dev",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "generate": "npm run build:dev && npm run build",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d @dist/prod --add"
    },
    "dependencies": {
        "@emotion/react": "^11.11.4",
        "@emotion/styled": "^11.11.5",
        "@mui/icons-material": "^5.16.4",
        "@mui/material": "^5.16.4",
        "@tanstack/react-query": "^5.49.2",
        "@tanstack/react-query-devtools": "^5.49.2",
        "@types/firebase": "^2.4.32",
        "@uiw/react-md-editor": "^4.0.4",
        "axios": "^1.6.8",
        "date-fns": "^3.6.0",
        "firebase": "^10.12.4",
        "framer-motion": "^11.3.18",
        "isomorphic-dompurify": "^2.4.0",
        "lodash": "^4.17.21",
        "node-sass": "^9.0.0",
        "react": "^18.2.0",
        "react-cookie": "^7.1.4",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.52.1",
        "react-router-dom": "^6.26.0",
        "react-secure-storage": "^1.3.2",
        "zustand": "^4.5.2"
    },
    "devDependencies": {
        "@types/lodash": "^4.17.7",
        "@types/node-forge": "^1.3.11",
        "@types/react": "^18.2.66",
        "@types/react-dom": "^18.2.22",
        "@typescript-eslint/eslint-plugin": "^7.2.0",
        "@typescript-eslint/parser": "^7.2.0",
        "@vitejs/plugin-react": "^4.2.1",
        "@vitejs/plugin-react-swc": "^3.7.0",
        "eslint": "^8.57.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-prettier": "^9.1.0",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-html": "^8.0.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-jsx-a11y": "^6.9.0",
        "eslint-plugin-react": "^7.28.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "eslint-plugin-react-refresh": "^0.4.5",
        "gh-pages": "^6.1.1",
        "msw": "^2.2.13",
        "node-forge": "^1.3.1",
        "prettier": "^3.3.3",
        "sass": "^1.71.0",
        "typescript": "^5.2.2",
        "vite": "^5.2.0",
        "vite-plugin-mkcert": "^1.17.5"
    },
    "msw": {
        "workerDirectory": [
            "public"
        ]
    }
}
